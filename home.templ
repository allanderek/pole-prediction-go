package main

templ Footer() {
	<footer class="bg-slate-900"></footer>
}

templ HomePage(isAuthenticated bool, fullname string) {
	@Page(
		"Pole prediction",
		"Predicting grand prix since, sometime this century",
		"/",
		isAuthenticated,
		fullname,
	) {
		<div class="bg-white">
			@Footer()
		</div>
	}
}
